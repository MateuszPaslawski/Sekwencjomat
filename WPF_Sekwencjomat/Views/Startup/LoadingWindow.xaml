<Window x:Class="WPF_Sekwencjomat.LoadingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Sekwencjomat" WindowStartupLocation="CenterOwner"
        mc:Ignorable="d" FontFamily="Google Sans" FontSize="13" Title="Przetwarzanie..." Loaded="Window_Loaded"
        SizeToContent="WidthAndHeight" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Label x:Name="Label_Status"/>
        <ProgressBar Grid.Row="1" Height="24" Width="250" Margin="5" IsIndeterminate="True"/>
    </Grid>
</Window>
