<Window x:Class="WPF_Sekwencjomat.SearchingVLCDLLsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_Sekwencjomat" Loaded="Window_Loaded" WindowStartupLocation="CenterScreen"
        mc:Ignorable="d" SizeToContent="WidthAndHeight" FontFamily="Google Sans" FontSize="13"
        Title="Poszukiwanie plików VLC">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Vertical">
                <Label x:Name="Label_Main" FontWeight="ExtraBold" HorizontalAlignment="Left" VerticalAlignment="Center"/>
            </StackPanel>
            <ProgressBar Height="20" Width="250" IsIndeterminate="True" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
